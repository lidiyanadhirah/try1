<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.a455l.try1.MainActivity">

    <Button
        android:id="@+id/banner1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/banner"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/textView4"
        app:layout_constraintRight_toRightOf="@+id/card"
        app:layout_constraintLeft_toLeftOf="@+id/card"
        app:layout_constraintTop_toTopOf="@+id/book"
        android:layout_marginTop="1dp" />

    <Button
        android:id="@+id/card"
        android:layout_width="123dp"
        android:layout_height="121dp"
        android:background="@drawable/card"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintRight_toLeftOf="@+id/copy"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/bunting"
        android:layout_marginEnd="19dp"
        android:layout_marginBottom="29dp"
        android:layout_marginRight="19dp" />

    <Button
        android:id="@+id/tshirt"
        android:layout_width="0dp"
        android:layout_height="121dp"
        android:background="@drawable/tshirt"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/copy"
        android:layout_marginTop="29dp"
        app:layout_constraintTop_toBottomOf="@+id/copy"
        app:layout_constraintLeft_toLeftOf="@+id/copy" />

    <Button
        android:id="@+id/book"
        android:layout_width="123dp"
        android:layout_height="121dp"
        android:background="@drawable/photobook"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        android:layout_marginEnd="61dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="42dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginRight="61dp" />

    <Button
        android:id="@+id/copy"
        android:layout_width="0dp"
        android:layout_height="121dp"
        android:background="@drawable/photostat"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/book"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@+id/book"
        app:layout_constraintLeft_toLeftOf="@+id/book" />

    <Button
        android:id="@+id/bunting"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/bunting"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/textView8"
        app:layout_constraintRight_toRightOf="@+id/card"
        app:layout_constraintLeft_toLeftOf="@+id/card"
        app:layout_constraintTop_toTopOf="@+id/tshirt" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="BANNER"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/card"
        android:layout_marginStart="34dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="3dp"
        app:layout_constraintLeft_toLeftOf="@+id/banner1"
        android:layout_marginLeft="34dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="PHOTOBOOK"
        tools:layout_constraintTop_creator="1"
        android:layout_marginStart="18dp"
        app:layout_constraintTop_toBottomOf="@+id/book"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/book"
        android:layout_marginLeft="18dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="CARD"
        tools:layout_constraintTop_creator="1"
        android:layout_marginStart="34dp"
        app:layout_constraintTop_toBottomOf="@+id/card"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/card"
        android:layout_marginLeft="34dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="PHOTOCOPY"
        tools:layout_constraintTop_creator="1"
        android:layout_marginStart="18dp"
        app:layout_constraintTop_toBottomOf="@+id/copy"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/copy"
        android:layout_marginLeft="18dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="BUNTING"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/textView9"
        android:layout_marginEnd="30dp"
        app:layout_constraintRight_toRightOf="@+id/bunting"
        app:layout_constraintTop_toBottomOf="@+id/tshirt"
        android:layout_marginRight="30dp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TSHIRT"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        android:layout_marginEnd="33dp"
        app:layout_constraintRight_toRightOf="@+id/tshirt"
        app:layout_constraintTop_toBottomOf="@+id/tshirt"
        android:layout_marginRight="33dp" />


</android.support.constraint.ConstraintLayout>
